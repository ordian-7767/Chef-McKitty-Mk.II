{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "no-console": 0,
        "import/no-extraneous-dependencies": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "class-methods-use-this": 0,
        "no-return-assign": 0,
        "no-prototype-builtins": 0,
        "no-new": 0,
        "no-undef": 0,
        "no-await-in-loop": 0,
        "no-plusplus": 0,
        "consistent-return" : 0,
        "import/prefer-default-export": 0,
        "import/no-unresolved": 0
    }
}
